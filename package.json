    "@subsquid/graphql-server": "3.1.0",
    "@subsquid/ss58": "0.1.2",
    "@subsquid/substrate-processor": "1.9.0",
    "@subsquid/typeorm-migration": "0.1.2",
    "@subsquid/typeorm-store": "0.1.3",
    "dotenv": "16.0.1",
    "ethers": "5.6.9",
    "pg": "8.7.3",
    "typeorm": "0.3.7"
